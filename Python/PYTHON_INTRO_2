l = [1, 3, 2]
print(len(l))
print(l)

l.append(4)
l.insert(2, 2)
print(l)

l.remove(1)
print(l)

l.sort()
print(l)

basket = {'apple', 'eggs', 'banana', 'orange'}
print(basket)

calories = {'apple' : 52, 'banana' : 80, 'choco' : 400}
print(calories)

c = calories
c['coffee'] = 98

print(c['apple'] < c['choco'])
print(c['banana'] < c['coffee'])

# Disctionary using keys() and values()
for k, v in calories.items():
    print(k) if v > 350 else None